{
  "src_files": [
    "src/**/*.js",
    "test/**/*.js"
  ],
  "launch_in_dev": ["Node"],
  "launch_in_ci": ["Node"],
  "launchers": {
    "Node": {
      "command": "node .testrun/test/test.js",
      "protocol": "tap"
    }
  },
  "before_tests": "npm run build-tests",
  "after_tests": "npm run clean-tests"
}
